{
  "name": "ytot",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-electron": "npm run build-ng && cp -a src/electron/. dist",
    "build-ng":"rm -r dist && ng build --base-href .",
    "electron": "npm run build-electron && electron dist",
    "build-win-release": "npm run build-electron && electron-packager ./dist YTOT --asar --overwrite --platform=win32 --arch=x64 --icon=resources/icon.ico --extra-resource=src/assets/Icons/icon.ico",
    "build-darwin-release": "npm run build-electron && electron-packager ./dist YTOT --asar --overwrite --platform=darwin --arch=x64",
    "build-linux-release": "npm run build-electron && electron-packager ./dist YTOT --asar --overwrite --platform=linux --arch=x64 --icon=resources/icon.ico --extra-resource=src/assets/Icons/icon.ico"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.5",
    "@angular/common": "^7.2.5",
    "@angular/compiler": "^7.2.5",
    "@angular/core": "^7.2.5",
    "@angular/forms": "^7.2.5",
    "@angular/http": "^7.2.5",
    "@angular/platform-browser": "^7.2.5",
    "@angular/platform-browser-dynamic": "^7.2.5",
    "@angular/router": "^7.2.5",
    "@types/youtube": "0.0.35",
    "core-js": "^2.5.7",
    "rxjs": "^6.4.0",
    "rxjs-compat": "^6.4.0",
    "zone.js": "^0.8.26",
    "@types/node": "^11.9.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.2",
    "@angular/cli": "^7.3.2",
    "@angular/compiler-cli": "^7.2.5",
    "@angular/language-service": "^7.2.5",
    "@types/jasmine": "~3.3.9",
    "@types/jasminewd2": "^2.0.5",
    "codelyzer": "~4.5.0",
    "electron": "^4.0.5",
    "electron-packager": "^13.1.0",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "^5.4.1",
    "ts-node": "~8.0.2",
    "tslint": "~5.12.1",
    "typescript": "^3.1.1"
  }
}
